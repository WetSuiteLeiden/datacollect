      <waarde>nummer 1;</waarde>
    </specificatie-item>
    <specificatie-item>
      <li.nr>b </li.nr>
      <specificatie>
        <al>de Christelijke Centrale van Overheids- en Onderwijspersoneel: </al>
      </specificatie>
      <waarde>nummer 2;</waarde>
    </specificatie-item>
    <specificatie-item>
      <li.nr>c </li.nr>
      <specificatie>
        <al>het Ambtenarencentrum: </al>
      </specificatie>
      <waarde>nummer 3</waarde>
    </specificatie-item>
    <specificatie-item>
      <li.nr>d </li.nr>
      <specificatie>
        <al>de Centrale van Middelbare en Hogere Functionarissen bij Overheid en Onderwijs: </al>
      </specificatie>
      <waarde>nummer 4.</waarde>
    </specificatie-item>
  </specificatielijst>
